{
  "name": "soiposervices/comfydeploy",
  "description": "A PHP client for the Comfy Deploy API",
  "keywords": ["comfy", "deploy", "php", "package"],
  "license": "MIT",
  "authors": [
    {
      "name": "Luigi Laezza",
      "email": "luigi@soiposervices.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "saloonphp/saloon": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "SoipoServices\\ComfyDeploy\\": "src/"
    }
